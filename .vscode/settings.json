{
    "dart.runPubGetOnPubspecChanges": "always",
    "cSpell.words": [
        "altive",
        "bluefireteam",
        "bools",
        "combinators",
        "diffscrape",
        "endtemplate",
        "futureor",
        "gocolly",
        "interps",
        "ints",
        "nonobvious",
        "pubspec",
        "redeclares",
        "RGBO",
        "riscait",
        "subosito",
        "Supertypes",
        "tearoffs",
        "todos",
        "unawaited",
        "writeln"
    ],
    "cSpell.ignorePaths": [
        "**/.pub-cache",
        "package-lock.json",
        "pubspec.lock",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders"
    ],
    "dart.showTodos": false,
    "markdown.extension.toc.levels": "2..3",
    "yaml.schemas": {
        "https://json.schemastore.org/yamllint.json": [
            "*.yaml",
        ]
    },
    "search.exclude": {
        "**/bower_components": true,
        "**/node_modules": true,
        "**/test/data": true,
        "**/*.code-search": true
    }
}